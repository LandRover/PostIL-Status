{
  "name": "postil-status",
  "version": "0.1.0",
  "description": "Israel's Post Package Status Tracker",
  "main": "src/postil.js",
  "devDependencies": {
    "blanket": "^1.2.1",
    "chai": "^3.4.1",
    "eslint": "^1.10.3",
    "mocha": "^2.3.3",
    "request-promise": "^2.0.0",
    "run-sequence": "^1.1.4",
    "sinon": "^1.17.2",
    "sinon-chai": "^2.8.0"
  },
  "dependencies": {
    "winston": "^2.1.1"
  },
  "config": {
    "blanket": {
      "pattern": [
        "src"
      ],
      "data-cover-never": "node_modules"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LandRover/postil-status.git"
  },
  "bugs": {
    "url": "https://github.com/LandRover/postil-status/issues"
  },
  "scripts": {
    "test": "mocha && mocha test --require blanket --reporter html-cov > coverage.html"
  },
  "keywords": [
    "postil",
    "packages",
    "tracking"
  ],
  "author": "Oleg Glozman",
  "license": "MIT"
}
